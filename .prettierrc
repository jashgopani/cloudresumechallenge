{
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "useTabs": false,
    "proseWrap": "never",
    "htmlWhitespaceSensitivity": "css"
}